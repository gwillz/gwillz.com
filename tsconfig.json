{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    // "outDir": "./",
    // "rootDir": "./",
    // "baseUrl": "./",
    "resolveJsonModule": true,
    "incremental": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "esModuleInterop": true
  }
}
